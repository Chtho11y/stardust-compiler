// recursive (Tower of Hanoi)

func hanoi(n: int, p1: int, p2: int, p3: int) -> int {
    if(n == 1){
        write(p1*1000000+p3);
    }
    else{
        hanoi(n-1,p1,p3,p2);
        write(p1*1000000+p3);
        hanoi(n-1,p2,p1,p3);
    }
    0
}

func main() -> int {
    let sum: int = 3;
    hanoi(sum, 1, 2, 3);
    0
}