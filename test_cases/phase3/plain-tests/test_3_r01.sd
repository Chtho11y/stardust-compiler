// judge whether the input is palindrome

func mod(x: int, n: int) -> int{
    let res = x -(x / n) * n;
    res
}

func isPalindrome(num: int) -> int {
    let reversed: int = 0;
    let original: int = num;
    let digit: int = 0;
    while (num != 0) {
        digit = mod(num , 10);
        reversed = reversed * 10 + digit;
        num = num / 10;
    }
    if(reversed == original){
        1
    }else{
        0 
    }
}

func main() -> int {
    let number: int;
    number = read(); 
    if (isPalindrome(number)==1) {
        write(1);
    } else {
        write(-1);
    }

    0
}